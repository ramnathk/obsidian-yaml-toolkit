{
	"name": "yaml-manipulator",
	"version": "0.1.0",
	"description": "Bulk manipulate YAML front matter with condition-based rules and preview",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"build:test": "npm run build && cp main.js manifest.json styles.css test-vault/.obsidian/plugins/yaml-manipulator/",
		"build:public": "node scripts/build-public.js",
		"docs:dev": "vitepress dev docs-site",
		"docs:build": "vitepress build docs-site",
		"docs:preview": "vitepress preview docs-site",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage",
		"test:e2e": "wdio run wdio.conf.js",
		"migrate:examples": "tsx scripts/migrate-examples-to-vitepress.ts",
		"generate:tests": "tsx tests/generators/generateFromExamples.ts",
		"e2e:download-obsidian": "node scripts/download-obsidian.js",
		"e2e:setup": "node scripts/e2e-setup.js",
		"prepare": "husky"
	},
	"keywords": [
		"obsidian",
		"yaml",
		"frontmatter",
		"bulk-edit"
	],
	"author": "ramnathk",
	"license": "MIT",
	"lint-staged": {
		"*.svelte": [
			"npm run build"
		],
		"src/**/*.ts": [
			"npm test"
		]
	},
	"dependencies": {
		"diff": "^8.0.2",
		"gray-matter": "^4.0.3",
		"js-yaml": "^4.1.1",
		"luxon": "^3.4.4"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^4.2.3",
		"@types/diff": "^7.0.2",
		"@types/js-yaml": "^4.0.9",
		"@types/luxon": "^3.4.2",
		"@types/node": "^20.0.0",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"@vitest/coverage-v8": "^1.0.4",
		"@vitest/ui": "^1.0.4",
		"@wdio/cli": "^9.2.8",
		"@wdio/local-runner": "^9.2.8",
		"@wdio/mocha-framework": "^9.2.8",
		"@wdio/spec-reporter": "^9.2.8",
		"builtin-modules": "3.3.0",
		"electron": "^33.2.0",
		"esbuild": "0.17.3",
		"esbuild-svelte": "^0.8.0",
		"husky": "^9.1.7",
		"jsdom": "^23.2.0",
		"lint-staged": "^16.2.7",
		"obsidian": "latest",
		"svelte": "^4.2.8",
		"tslib": "2.4.0",
		"tsx": "^4.7.0",
		"typescript": "4.7.4",
		"vitepress": "^1.6.4",
		"vitest": "^1.0.4",
		"wdio-electron-service": "^9.2.1"
	}
}
